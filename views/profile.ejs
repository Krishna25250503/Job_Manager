<%- include('partials/header')%>

    <%- include('partials/nav')%>
        <br>
        <br>
        <div class="container text-light bg-dark ">
            <h1 class="m-auto d-flex text-align-right list-group-item border-0 mb-3 d-flex justify-content-center   ">All Profile's</h1>
            <br>
            <h3 class="text-warning list-group-item border-0 mb-3 d-flex justify-content-center">Welcome to Profile Page</h3>
            <hr />
            <h3 class=" border-0 mb-3 d-flex justify-content-center">List Of All Users</h3>
            <ul class="m-auto list-group w-25">
                <% users.forEach((user)=> { %>
                    <li class="list-group-item border-0 mb-3 d-flex justify-content-between">
                        <%= user.username%>
                            <span>
                                <a class="text-decoration-none" href="/update/<%= user._id%>">✏️ Create</a>
                                <a class="text-decoration-none" href="/delete/<%= user._id%>">❌ Delete</a>
                            </span>
                    </li>
                    <%})%>
            </ul>
        </div>

        <div class="p-3 mb-2 bg-dark text-white ">
            <div class="p-3 mb-2 bg-dark text-white ">
                <div class="p-3 mb-2 bg-dark text-white ">
                    <div class="p-3 mb-2 bg-dark text-white ">
                        <div class="p-3 mb-2 bg-dark text-white ">


                            <%- include('partials/footer')%>